{
	"name": "microservices",
	"private": true,
	"type": "module",
	"devDependencies": {
		"lerna": "^4.0.0"
	},
	"prettier": {
		"singleQuote": true,
		"trailingComma": "all",
		"semi": false,
		"tabWidth": 2
	},
	"eslintConfig": {
		"extends": [
			"plugin:prettier/recommended",
			"plugin:import/errors",
			"plugin:import/warnings",
			"plugin:security/recommended"
		],
		"plugins": [
			"prettier",
			"import",
			"security"
		],
		"parserOptions": {
			"sourceType": "module",
			"ecmaFeatures": {
				"modules": true
			},
			"ecmaVersion": 2020
		},
		"env": {
			"node": true,
			"es6": true
		},
		"rules": {
			"import/extensions": [
				"error",
				"always",
				{
					"ignorePackages": true
				}
			]
		}
	},
	"dependencies": {
		"@grpc/grpc-js": "^1.2.12",
		"@grpc/proto-loader": "^0.6.0",
		"autoprefixer": "^10.2.5",
		"ava": "^3.15.0",
		"c8": "^7.7.1",
		"consola": "^2.15.3",
		"core-js": "^3.10.1",
		"dayjs": "^1.10.4",
		"eslint": "^7.24.0",
		"eslint-config-prettier": "^8.1.0",
		"eslint-plugin-import": "^2.22.1",
		"eslint-plugin-jsx-a11y": "^6.4.1",
		"eslint-plugin-prettier": "^3.3.1",
		"eslint-plugin-react": "^7.23.2",
		"eslint-plugin-react-hooks": "^4.2.0",
		"eslint-plugin-security": "^1.4.0",
		"fastify": "^3.14.2",
		"fastify-compress": "^3.4.2",
		"fastify-helmet": "^5.3.1",
		"fastify-metrics": "^7.1.0",
		"fastify-rate-limit": "^5.5.0",
		"fastify-sensible": "^3.1.1",
		"fastify-swagger": "^4.5.1",
		"knex": "^0.95.4",
		"mali": "^0.41.2",
		"mobx": "^6.2.0",
		"next": "10.1.3",
		"next-seo": "^4.23.0",
		"pg": "^8.5.1",
		"postcss": "^8.2.10",
		"prettier": "^2.2.1",
		"quibble": "^0.6.5",
		"react": "17.0.2",
		"react-dom": "17.0.2",
		"redoc": "^2.0.0-rc.53",
		"styled-components": "^5.2.3",
		"tailwindcss": "^2.1.1",
		"under-pressure": "^5.6.0",
		"uuid": "^8.3.2"
	}
}
